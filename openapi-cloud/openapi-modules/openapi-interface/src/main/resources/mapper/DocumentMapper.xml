<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zty.interfaces.mapper.DocumentMapper">

    <select id="titleList" resultType="com.zty.interfaces.vo.DocumentTitleVO">
        SELECT id,title FROM `document` where is_deleted = 0 ORDER BY sort ASC
    </select>

</mapper>
